.home{
      padding: 0 30rpx 30rpx;
      font-size: 26rpx;
}
.home .search .van-search__content {
    border-radius: 34rpx;

}
.home .banner{
  width: 690rpx;
  border-radius: 20rpx;
}
.home .msg_box{
  background: #F0F0F0;
  padding: 0 12rpx;
  font-size: 24rpx;
}
.home .msg_height{
  height: 55rpx;
  line-height: 55rpx;
}
.home .msg_text{
  color: #E74A4A;
  position: relative;
}
.home .msg_text::after{
  position: absolute;
  top:15%;
  right: 0;
  width: 2rpx;
  height: 70%;
  content: "";
  background: #999;
}
