.signIn {
    padding: 0 30rpx 30rpx;
    min-height: 100vh;
    box-sizing: border-box;
    font-size: 26rpx;
    color: #333;
}

.signin_title {
    height: 52rpx;
    line-height: 52rpx;
    background: rgba(240, 240, 240, 1);
    padding-left: 20rpx;
    box-sizing: border-box;
}

.sign_in_img {
    width: 46rpx;
    height: 46rpx;
    margin-right: 20rpx;
}

.sign_in_item {
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    align-items: center;
    height: 80rpx;
}

.sign_in_left {
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
}

.sign_in_doc {
    width: 8rpx;
    height: 8rpx;
    background: rgba(232, 56, 13, 1);
    border-radius: 50%;
    margin-right: 20rpx;
}

.sign_gray {
    color: #999;
}

.sign_red {
    color: #E8380D;
}
.sign_in_btns{
    position: absolute;
    bottom: 50rpx;
    left: 20rpx;
    width: 690rpx;
    display: flex;
    flex-flow: row nowrap;
    border-radius:10px;
    overflow: hidden;
}
.sign_btn{
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 26rpx;
    color: white;
    flex-grow: 1;
}
.btns_green{
    background:rgba(9,201,162,1);
}
.btns_red{
    background:rgba(242,103,70,1);
}