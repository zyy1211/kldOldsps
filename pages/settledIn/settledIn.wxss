.settledIn {
    padding-bottom: 50rpx;
}

.settledIn .set_item {
    color: #333;
    padding: 0 60rpx;
}

.set_font_24 {
    font-size: 26rpx;
}

.bottom_20 {
    margin-bottom: 20rpx;
    margin-top: 40rpx;
    font-weight: 800;
}

.set_block {
    height: 104rpx;
    border: 1rpx solid rgba(153, 153, 153, 1);
    border-radius: 10rpx;
    position: relative;
    color: #999;
}

.like-input {
    position: absolute;
    top: 1%;
    left: 0;
    height: 98%;
    padding: 0 20rpx;
    box-sizing: border-box;
    /* border: 1px solid red; */
    width: 100%;
}
.item_icon{
    position: absolute;
    top: 0;
    right: 20rpx;
    height: 100%;
}

.notify {
    font-size: 24rpx;
    color: #F26746;
    margin: 0 30rpx;
    box-sizing: border-box;
    padding: 0 30rpx;
    height: 70rpx;
    background: rgba(240, 240, 240, 1);
}

.notify_icon {
    width: 34rpx;
    height: 34rpx;
    margin-right: 30rpx;
}

.time_block {
    width: 120rpx;
    height: 50rpx;
    line-height: 50rpx;
    border: 1rpx solid rgba(176, 176, 176, 1);
    border-radius: 10rpx;
    text-align: center;
}
.line_row{
    padding: 0 20rpx;
    line-height: 45rpx;
}
.item_block_item{
    height: 300rpx;
    border: 1rpx solid rgba(176, 176, 176, 1);
    border-radius: 10rpx;
    position: relative;
    box-sizing: border-box;
    overflow:hidden;
}
.img_add{
    width: 96rpx;
}
.text_add{
    font-size: 24rpx;
    color: #999;
    margin-top: 30rpx;
}
.total_add{
    position: absolute;
    right: 30rpx;
    bottom: 20rpx;
}
.img_list{
    padding: 30rpx 0 30rpx 4%;
}

.img_item{
    width: 16%;
    height:120rpx;
    box-sizing: border-box;
    margin-right: 4%;
    border: 1rpx solid #999;
    position: relative;
    border:1px solid #999;
}
.img_item image{
    width: 100%;
    height:100%;
}
.del_icon{
    position: absolute;
    right: -10rpx;
    top: -10rpx;
}
.add_tab{
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    background-color: #999;
    color: white;
    margin-bottom: 25rpx;
    margin-right:6rpx;
    border-radius: 10rpx;
    font-size: 24rpx;
    text-align: center;
}
.add_tab_active{
    background-color: #F26746;
}
.btn{
    display: block;
    margin: 0 30rpx;
    box-sizing: border-box;
    height: 72rpx;
    line-height: 72rpx;
    border-radius: 10rpx;
    text-align: center;
    color: white;
    margin-top: 80rpx;
}
.red_btn{
    background-color: #F26746;
}
.green_btn{
    background-color: #09C9A2;
}
