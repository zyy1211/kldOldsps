.venuesDetail {
    padding: 0 30rpx;
    color: #333;
    padding-bottom: 300rpx;
}

.mainText {
    background-color: white;
    border-bottom-right-radius: 20rpx;
    border-bottom-left-radius: 20rpx;
    box-shadow: 8rpx 8rpx 10rpx 0rpx rgba(4, 0, 0, 0.23);
    padding: 70rpx 20rpx 22rpx;
    position: relative;
}

.mainText .title_block {
    position: absolute;
    width: 100%;
    padding: 0 36rpx;
    box-sizing: border-box;
    top: -30rpx;
    left: 0;
}

.mainText .title {
    height: 86rpx;
    line-height: 86rpx;
    border-radius: 43rpx;
    padding: 0 20rpx;
    font-size: 34rpx;
    color: white;
    font-weight: 900;
    background-color: #7ECEF4;

}
.cardimg{
  width: 246rpx;
  border-radius: 10rpx;
}
.cardName{
  position: absolute;
  top: 36rpx;
  left: 0;
  width: 246rpx;
  color: white;
  font-size: 32rpx;
  font-weight: 900;
  text-align: center;
}
.cardTitle{
  font-size: 32rpx;
  font-weight: 900;
  margin: 30rpx 0;
}

.mainText .detail {
    font-size: 24rpx;
    color: #666;
    line-height: 36rpx;
}

.mainText .address {
    font-size: 26rpx;
    color: #333;
    font-weight: 900;
    margin: 40rpx 0;
}

.mainText .phone {
    font-size: 26rpx;
    color: #333;
}

.mainText .tabsBlock {
    margin-top: 40rpx;
    /* border: 1px solid red; */
}

.mainText .tabs {
    font-size: 24rpx;
    margin-right: 20rpx;
    margin-bottom: 22rpx;
}

.typeTabs {
    font-size: 28rpx;
    color: #666;
    margin-right: 20rpx;
    position: relative;
    padding: 20rpx 20rpx 0 0;
    /* border: 1px solid red; */
}

.tabsActive {
    color: #333;
    font-size: 32rpx;
    font-weight: 900;
}

.tabsActive::after {
    position: absolute;
    bottom: 0rpx;
    left: 0;
    height: 4rpx;
    width: 60%;
    content: '';
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    background-color: #09C9A2;
}

.tabsList {
    /* margin: 50rpx 0; */
    height: 100rpx;
}

.daytext {
    width: 90rpx;
    height: 90rpx;
    border-radius: 10rpx;
    font-size: 24rpx;
    padding: 14rpx 0;
    box-sizing: border-box;
    color: #999;
    background-color: white;
    border: 1rpx solid rgba(153, 153, 153, 1);
}

.triangle {
    width: 0;
    height: 0;
    border: 14rpx solid transparent;
    margin-left: 30rpx;
    border-top-color: transparent;
}

.t_active {
    border-top-color: rgba(242, 103, 70, 0.8);
}

.filedMark {
    display: flex;
    flex-flow: row nowrap;
    justify-content: flex-start;


    /* padding-top: 10rpx; */
}


.markBlock{
    margin: 5rpx;
    position: relative;
    /* border: 1rpx solid red; */

}
.contM{
  flex-grow: 1;
  /* overflow-x: scroll; */
  display: flex;
  flex-flow: row nowrap;
  width: 570rpx;
  /* border: 1rpx solid red; */
}
.filedMark .mark {
    width: 160rpx;
    font-size: 24rpx;
    /* margin-bottom: 20rpx; */
    /* margin-top: 45rpx; */


}



.filedMark .block {
    width: 160rpx;
    height: 60rpx;
    line-height: 60rpx;
    border: 1rpx solid rgba(102, 102, 102, 1);
    border-radius: 5rpx;
    margin-bottom: 20rpx;
    position: relative;
    box-sizing: border-box;
}
.filedMark .disable{
    background-color:rgba(0, 0, 0,0.2);
    border: none;
}
.filedMark .timeblock{
    width: 100rpx;
    height: 60rpx;
    margin-bottom: 20rpx;
    position: relative;
    /* border: 1rpx solid blue; */
}

.descList{
    display: flex;
    flex-flow: column nowrap;
    justify-content: flex-start;

}
.filedMark .timeText{
  position: absolute;
  top: -24rpx;
  right: 20rpx;
  text-align: right;
  font-size: 24rpx;
  /* width: 100%; */
}
.bottomFixed{
    position: fixed;
    bottom: 0rpx;
    left: 0;
    background-color: white;
    padding-bottom: 10rpx;
}
.isImg{
    width: 35rpx;
    height: 35rpx;
  }
.selectL{
    width: 750rpx;
    height: 280rpx;
    z-index: 99;
    /* border: 1px solid red; */
  }
  .select-ul{
    width: 750rpx;
    height: 120rpx;
    padding: 20rpx 0;
    white-space: nowrap;
  }
  .yd,.select-li{
    margin: 0 20rpx;
    display: inline-block;
    font-size: 26rpx;
  }
  .yd{
    width:140rpx;
    height: 80%;
  }
  .select-li{
    /* width:160rpx; */
    height: 90rpx;
    margin-top:10rpx;
    border:1rpx solid #F26746;
    color:#F26746;
    border-radius: 7rpx;
    padding: 0 20rpx;

  }
  .rect{
    width:80%;
    height: 60rpx;
    margin:10rpx auto;
    border-radius: 7rpx;
  }
  .isTrue{
    background: #F26746;
    border: 1rpx solid rgba(0, 0, 0,0);
  }
  .unKnow{
    border: 1rpx solid rgba(0, 0, 0,0.2);
  }
  .saled{
    background: rgba(0, 0, 0,0.2);
    border: 1rpx solid rgba(0, 0, 0,0);
  }
  .Y{
    width: 100%;
    height: 50%;
  }
  .btn{
    width: 700rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 10rpx;
  }
  .btnActive{
    color:#fff;
    background:#F26746;
  }
  .active {
    color: white;
    background-color: rgba(242, 103, 70, 0.8);
    border: none!important;
}