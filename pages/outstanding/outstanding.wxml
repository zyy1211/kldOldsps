<view class="outstanding">
    <view class="ots_tabs">
        <view bindtap="changeTabs" data-key="0" class="ots_ing {{active==0 ? 'ots_active' : ''}}">待结算金额</view>
        <view bindtap="changeTabs" data-key="1" class="ots_ed {{active==1 ? 'ots_active' : ''}}">已结算金额</view>
    </view>
    <view class="ots_list">
        <block wx:for="{{list}}">
            <view class="ots_item">
                <view>
                    <view class="ots_key">活动名称：</view>
