                </view>
                <view>
                    <view class="ots_key">付款时间：</view>
                    <view class="ots_value">2019-10-30 19:35:11</view>
                </view>
                <view>
                    <view class="ots_key">付款单号：</view>
                    <view class="ots_value">51221133454236</view>
                </view>
                <view>
                    <view class="ots_key">付款人名：</view>
                    <view class="ots_value">吴彦祖本租</view>
                </view>
                <view>
                    <view class="ots_key">待付金额：</view>
                    <view class="ots_value ots_money">50元</view>
                </view>
            </view>
        </block>
    </view>
</view>